<% provide(:title, item_type.name_plural) %>

<% content_for :top_content do %>
  <div class="top-content-type-generic">
    <%= render("shared/searchlinks") %>
  </div>
<% end %>

<div id="main-content">
  <div id="main-wrapper">
    <div id="main">
      <% if @corpus.present? %>
        <%= render("items/type/corpus") %>
      <% end %>

      <% if @domain.present? %>
        <div id="ouvrage">
          <span class="searchmsg">
            <%= @browse.total_count %> <%= t(".images-for-domain") %>: <i>«<%= @domain %>»</i> 
          </span>
        </div>
      <% end %>

      <%= render_item_list(@browse) %>
      <%= paginate(@browse.items) %>
    </div>
  </div>
</div>
